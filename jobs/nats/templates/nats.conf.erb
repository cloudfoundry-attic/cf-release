net: "<%= spec.networks.send(properties.networks.apps).ip %>"
port: <%= p("nats.port") %>

pid_file: "/var/vcap/sys/run/nats/nats.pid"
log_file: "/var/vcap/sys/log/nats/nats.log"

authorization {
  user: "<%= p("nats.user") %>"
  password: "<%= p("nats.password") %>"
  timeout: <%= p("nats.authorization_timeout") %>
}
